{
  "name": "amazon-ynab-sync",
  "version": "1.0.0",
  "description": "Amazon to YNAB Sync with React UI",
  "main": "server.ts",
  "scripts": {
    "dev": "npm run server & npm run client",
    "server": "npx ts-node server.ts",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "login": "npx ts-node login.ts",
    "sync": "npx ts-node transactions-to-json.ts",
    "ynab": "npx ts-node ynab-sync.ts"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "openai": "^4.59.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "concurrently": "^8.0.0"
  }
}
